{
    "builds": [{
        "src": "ai_running_partner_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ai_running_partner_api/wsgi.py"
        }
    ]
}